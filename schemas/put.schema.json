{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "success",
    "content"
  ],
  "properties": {
    "success": {
      "type": "boolean"
    },
    "content": {
      "type": "object",
      "required": [
        "id",
        "description",
        "price"
      ],
      "properties": {
        "id": {
          "type": "integer"
        },
        "description": {
          "type": "string"
        },
        "price": {
          "type": "number"
        }
      }
    }
  }
}
